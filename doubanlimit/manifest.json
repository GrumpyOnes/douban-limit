{
    "manifest_version": 2,
    "name": "douban limit",
    "version": "1.0",
    "description": "Limit your Douban time.",
    "icons":{
        "16":"images/icon-19.png",
		"48":"images/icon-48.png",
		"128":"images/icon-128.png"
    },
	"background": { "scripts": ["js/dbl-background.js"] },
	"permissions" : [
		"tabs",
		"notifications"
	],
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-48.png"
        },
        "default_title": "Douban Limit",
        "default_popup": "popup.html"
    }
}